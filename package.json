    "d3": "~3.5.17",
    "datatables.net": "~2.0.0",
    "datatables.net-dt": "~1.13.10",
    "jquery": "~3.7.1",
    "jsonview": "~1.2.0",
    "spin": "~0.0.1",
    "split.js": "~1.6.4",
    "underscore": "~1.13.1",
    "webpack-jquery-ui": "~2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^5.2.7",
    "eslint": "^8.39.0",
    "file-loader": "^6.2.0",
    "qunit": "2.20.0",
    "style-loader": "^3.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-merge": "^5.8.0"
